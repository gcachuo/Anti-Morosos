<?xml version='1.0' encoding='utf-8'?>
<widget id="com.kchusoftware.antimorosos" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>AntiMorosos</name>
    <description>
        AntiMorosos
    </description>
    <author email="guillermocachu06@gmail.com" href="http://github.com/gcachuo">
       Guillermo Cach√∫ Osorio
    </author>
    <content src="index.html" />
    <preference name="DisallowOverscroll" value="true" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="android-minSdkVersion" value="19" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-inappbrowser" spec="~1.7.1" />
    <engine name="browser" spec="~5.0.4" />
</widget>
